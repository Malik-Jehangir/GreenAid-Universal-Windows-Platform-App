<Page
    x:Class="GreenAid.BlankPage9"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:local="using:GreenAid"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    mc:Ignorable="d"
    Background="{ThemeResource ApplicationPageBackgroundThemeBrush}" Height="797.884">
    <Page.Resources>
        <Thickness x:Key="PhoneBorderThickness">2.5</Thickness>
        <FontFamily x:Key="PhoneFontFamilyNormal">Segoe WP</FontFamily>
        <FontWeight x:Key="PhoneButtonFontWeight">Semibold</FontWeight>
        <x:Double x:Key="TextStyleLargeFontSize">18.14</x:Double>
        <Thickness x:Key="PhoneButtonContentPadding">9.5,0,9.5,3.5</Thickness>
        <x:Double x:Key="PhoneButtonMinHeight">57.5</x:Double>
        <x:Double x:Key="PhoneButtonMinWidth">109</x:Double>
        <Thickness x:Key="PhoneTouchTargetOverhang">0,9.5</Thickness>
        <SolidColorBrush x:Key="ButtonDisabledBackgroundThemeBrush" Color="Transparent"/>
    </Page.Resources>
    <Page.BottomAppBar>
        <CommandBar Background="#FF4960CD">
            <AppBarButton x:ConnectionId='1' Icon="Back" Label="Back"                     />
        </CommandBar>
    </Page.BottomAppBar>

    <ScrollViewer Margin="0,0,0,0.167" Grid.RowSpan="2">
        <Grid Background="#FFEFF9F8" Height="1373">
            <Grid.RowDefinitions>
                <RowDefinition Height="*"/>
                <RowDefinition Height="10"/>
            </Grid.RowDefinitions>
            <Grid HorizontalAlignment="Left" Height="60" VerticalAlignment="Top" Width="450" Background="#FF4960CD">
                <TextBlock HorizontalAlignment="Left" TextWrapping="Wrap" Text="Our Experts " VerticalAlignment="Top" Height="39" Width="168" Margin="171,17,0,0" FontSize="24" Foreground="#FFF9F5F5" FontWeight="Bold"/>
            </Grid>
            <Image x:ConnectionId='2'   HorizontalAlignment="Left" VerticalAlignment="Top" Margin="19,80,0,0" Height="138" Width="130"                         Source="Assets/Martina Newell-McGloughlin.JPG" Stretch="UniformToFill">


            </Image>
            <Image x:ConnectionId='3' HorizontalAlignment="Left" VerticalAlignment="Top" Margin="19,640,0,0" Height="138" Width="130"                         Source="Assets/Lemaux.jpg" Stretch="UniformToFill">

            </Image>
            <Image x:ConnectionId='4'  HorizontalAlignment="Left" VerticalAlignment="Top" Margin="19,258,0,0" Height="138" Width="130"                         Source="Assets/trewavas.jpg" Stretch="UniformToFill">
            </Image>
            <TextBlock HorizontalAlignment="Left" TextWrapping="Wrap" VerticalAlignment="Top" Height="42" Width="202" Margin="162,84,0,0" FontSize="24" Foreground="#FF060202" FontWeight="Bold" Text="Martina Newell"/>
            <TextBlock HorizontalAlignment="Left" TextWrapping="Wrap" VerticalAlignment="Top" Height="53" Width="246" Margin="162,257,0,0" FontSize="24" Foreground="#FF151212" FontWeight="Bold" Text="Anthony Trevawas"/>
            <TextBlock HorizontalAlignment="Left" TextWrapping="Wrap" VerticalAlignment="Top" Height="32" Width="172" Margin="162,643,0,0" FontSize="24" Foreground="#FF190A0A" FontWeight="Bold" Text="Peggy Lemaux"/>
            <Image x:ConnectionId='5' HorizontalAlignment="Left" VerticalAlignment="Top" Margin="19,827,0,0" Height="138" Width="130"                         Source="Assets/Ingo_Potrykus.png" Stretch="UniformToFill">


            </Image>
            <Image x:ConnectionId='6'  HorizontalAlignment="Left" VerticalAlignment="Top" Margin="19,449,0,0" Height="138" Width="130"                         Source="Assets/Yo2012.jpg" Stretch="UniformToFill">

            </Image>
            <TextBlock HorizontalAlignment="Left" TextWrapping="Wrap" VerticalAlignment="Top" Height="32" Width="240" Margin="168,828,0,0" FontSize="24" Foreground="#FF130E0E" FontWeight="Bold">
        	<Run FontWeight="Bold" Text="Ingo Potrykus"/>
            </TextBlock>
            <TextBlock HorizontalAlignment="Left" TextWrapping="Wrap" VerticalAlignment="Top" Height="32" Width="172" Margin="162,454,0,0" FontSize="24" Foreground="#FF110909" FontWeight="Bold" Text="Wayne Parrott"/>
            <Image x:ConnectionId='7'  HorizontalAlignment="Left" VerticalAlignment="Top" Margin="19,1019,0,0" Height="138" Width="130"                         Source="Assets/wambugu.jpg" Stretch="UniformToFill">

            </Image>
            <TextBlock HorizontalAlignment="Left" TextWrapping="Wrap" VerticalAlignment="Top" Height="53" Width="236" Margin="168,1017,0,0" FontSize="24" Foreground="#FF110E0E" FontWeight="Bold" Text="Florence Wambugu"/>
            <TextBlock HorizontalAlignment="Left" TextWrapping="Wrap" VerticalAlignment="Top" Height="162" Width="226" Margin="162,120,0,0" FontSize="22" Foreground="#FF060202" FontWeight="Bold">
            	<Run FontWeight="Normal" Text="Martina Newell-McGloughlin, PhD. Biotechnology. UC Davis. Director,"/>
            </TextBlock>
            <TextBlock HorizontalAlignment="Left" TextWrapping="Wrap" VerticalAlignment="Top" Height="102" Width="254" Margin="162,294,0,0" FontSize="22" Foreground="#FF060202" FontWeight="Bold">
            	<Run FontWeight="Normal" Text="Professor at the Institute of Cell and Molecular Biology of the University of Edinburgh"/>
            </TextBlock>
            <TextBlock HorizontalAlignment="Left" TextWrapping="Wrap" VerticalAlignment="Top" Height="102" Width="254" Margin="162,492,0,0" FontSize="22" Foreground="#FF060202" FontWeight="Bold">
            	<Run FontWeight="Normal" Text="Professor of crop sciences at the University of Georgia in Athens."/>
            </TextBlock>
            <TextBlock HorizontalAlignment="Left" TextWrapping="Wrap" VerticalAlignment="Top" Height="102" Width="254" Margin="168,684,0,0" FontSize="22" Foreground="#FF060202" FontWeight="Bold">
            	<Run FontWeight="Normal" Text="Specialist in the Department of Plant and Microbial Biology"/>
            </TextBlock>
            <TextBlock HorizontalAlignment="Left" TextWrapping="Wrap" VerticalAlignment="Top" Height="190" Width="254" Margin="168,870,0,0" FontSize="22" Foreground="#FF060202" FontWeight="Bold">
            	<Run FontWeight="Normal" Text="Professor Emeritus of Plant Sciences at the Institute of Plant Sciences Zurich"/>
            	<Run Text=" "/>
            </TextBlock>
            <TextBlock HorizontalAlignment="Left" TextWrapping="Wrap" VerticalAlignment="Top" Height="107" Width="254" Margin="168,1062,0,0" FontSize="22" Foreground="#FF060202" FontWeight="Bold">
            	<Run FontWeight="Normal" Text="She is known for her advocacy of using biotechnology to increase food production in Africa"/>
            </TextBlock>

        </Grid>
    </ScrollViewer>
</Page>

